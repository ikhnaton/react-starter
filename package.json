{
  "name": "react-starter",
  "version": "1.0.1",
  "description": "React Starter Project",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikhnaton/react-starter.git"
  },
  "scripts": {
    "build": "node_modules/.bin/webpack --config scripts/webpack.prod.config.js",
    "serverbuild": "node_modules/.bin/webpack --config scripts/webpack.serverOnly.config.js --watch",
    "start": "node dist/server.js",
    "dev": "concurrently --names \"nodemon,webpack\" -c \"blue,green\" \"npm run devClient\" \"npm run devServer\"",
    "devClient": "node_modules/.bin/webpack --config scripts/webpack.config.js",
    "server": "concurrently --names \"nodemon,webpack\" -c \"blue,green\" \"npm run serverbuild\" \"npm run devServer\"",
    "devServer": "nodemon -w dist/server.js dist/server.js",
    "debug": "node_modules/.bin/webpack serve --config scripts/webpack.config.js",
    "devall": "node_modules/.bin/webpack serve --config scripts/webpack.config.js --host 0.0.0.0",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts ./src",
    "lint-fix": "eslint --ext .jsx,.js,.tsx,.ts ./src --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "validate": "npm run lint-fix && npm run test"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 12
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "verbose": true,
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.less$": "identity-obj-proxy",
      "^common(.*)$": "<rootDir>/src/common$1",
      "^modules(.*)$": "<rootDir>/src/modules$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^root(.*)$": "<rootDir>/src$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {}
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "testMatch": [
    "**/__tests__/**/*.+(ts|tsx|js)",
    "**/?(*.)+(spec|test).+(ts|tsx|js)"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/carbon-components-react": "^7.36.1",
    "@types/jest": "^27.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^8.0.7",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "bufferutil": "^4.0.3",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-sonarjs": "^0.10.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "sass-loader": "^12.1.0",
    "sonarjs": "^1.0.0",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@koa/router": "^10.1.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.3",
    "carbon-components": "^10.41.0",
    "carbon-components-react": "^7.41.0",
    "carbon-icons": "^7.0.7",
    "date-fns": "^2.23.0",
    "graphql": "^15.5.2",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.4",
    "node-cache": "^5.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^3.0.15",
    "react-json-pretty": "^2.2.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "saslprep": "^1.0.3"
  }
}
